        <characteristic name="Save" typeId="055d-0b88-bbaf-dc8c">4+</characteristic>
        <characteristic name="CAF" typeId="fed2-08a4-b3f9-f4f3">+0</characteristic>
        <characteristic name="Weapons" typeId="8e21-035a-6cdc-67eb">Pulsa Rokkit - Special</characteristic>
        <characteristic name="Morale" typeId="3bb5-71b6-c61b-07da">4+</characteristic>
