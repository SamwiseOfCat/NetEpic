        <characteristic name="Special Rules" typeId="d338-64c1-8de3-aa2b"></characteristic>
      </characteristics>
    </profile>
    <profile id="cfd0-5c80-8c9d-da3f" name="Solar Flare - Focused Level" hidden="false" typeId="5857-5233-17f1-2389" typeName="Solar Flare Attacks">
      <characteristics>
        <characteristic name="Plasma Counters Required" typeId="3caa-992e-dc12-4cd1">2</characteristic>
        <characteristic name="Range" typeId="f9b1-e605-ac31-5908">120cm</characteristic>
